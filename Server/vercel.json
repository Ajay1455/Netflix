{
    "version": 2,
    "functions": {
        "/**/*": {
            "timeout": 30
        }
    },
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}